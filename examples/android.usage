android [--silent|--verbose]
  ( list [avd|target]
  | create avd ( --target <name> | --name <name> | --skin <name> | --path <file>
               | --sdcard <file> | --force ) ...
  | move avd (--name <name> | --rename <name> | --path <file>) ...
  | (delete|update) avd --name <name>
  | create project ((--package|--name|--activity|--target) <val> | --path <file>) ...
  | update project ((--target|--name) <val> | --path <file>) ...
  | update adb )
|
android --help
